      <Association Name="Developer_Game" Member="Games" ThisKey="Id" OtherKey="DeveloperId" Type="Game" />
    </Type>
  </Table>
  <Table Name="dbo.mgdf_users" Member="Users">
    <Type Name="User">
      <Column Name="Id" Type="System.Guid" DbType="UniqueIdentifier NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
