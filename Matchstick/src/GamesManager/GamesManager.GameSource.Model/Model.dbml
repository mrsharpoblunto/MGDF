      <Column Name="GameVersionId" Type="System.Guid" CanBeNull="false" />
      <Column Name="PublishOnComplete" Type="System.Boolean" DbType="Bit NOT NULL" CanBeNull="false" />
      <Column Name="CreatedDate" Type="System.DateTime" CanBeNull="false" />
      <Association Name="GameFragment_GameVersion" Member="GameVersion" ThisKey="GameVersionId" OtherKey="Id" Type="GameVersion" Cardinality="One" />
      <Association Name="Developer_GameFragment" Member="Developer" ThisKey="DeveloperId" OtherKey="Id" Type="Developer" IsForeignKey="true" />
    </Type>
  </Table>
  <Table Name="dbo.mgdf_developers" Member="Developers">
    <Type Name="Developer">
      <Column Name="Id" Type="System.Guid" DbType="UniqueIdentifier NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
