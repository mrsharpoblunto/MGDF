      <Column Name="Description" Type="System.String" DbType="VarChar(255) NOT NULL" CanBeNull="false" />
      <Column Name="Uid" Type="System.String" DbType="VarChar(255) NOT NULL" CanBeNull="false" />
      <Column Name="Homepage" Type="System.String" DbType="VarChar(255) NOT NULL" CanBeNull="false" />
      <Column Name="DeveloperId" Type="System.Guid" DbType="UniqueIdentifier" CanBeNull="false" />
      <Column Name="InterfaceVersion" Type="System.Int32" DbType="Int" CanBeNull="false" />
      <Column Name="RequiresAuthentication" Type="System.Boolean" DbType="Bit NOT NULL" CanBeNull="false" />
      <Association Name="Game_UserGame" Member="UserGames" ThisKey="Id" OtherKey="GameId" Type="UserGame" Cardinality="One" />
      <Association Name="Game_GameVersion" Member="GameVersions" ThisKey="Id" OtherKey="GameId" Type="GameVersion" />
