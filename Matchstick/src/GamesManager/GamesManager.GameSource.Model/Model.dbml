<?xml version="1.0" encoding="utf-8"?>
<Database Name="GameSource" Class="GameSourceRepository" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Connection Mode="ConnectionString" ConnectionString="Data Source=(local);Persist Security Info=True;User ID=dev" Provider="System.Data.SqlClient" />
  <Table Name="dbo.mgdf_gamefragments" Member="GameFragments">
    <Type Name="GameFragment">
      <Column Name="Id" Type="System.Guid" DbType="UniqueIdentifier NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="GameDataId" Storage="_GameFragmentFile" Type="System.String" DbType="VarChar(255)" CanBeNull="true" />
      <Column Name="DeveloperId" Type="System.Guid" DbType="UniqueIdentifier" CanBeNull="false" />
